<ul class="nav nav-pills">
    {% for account, data in accountData %}
        <li role="presentation" {% if loop.first %}class="active"{% endif %}><a href="#raw-data-{{ loop.index0 }}">{{ account }}</a></li>
    {% endfor %}
</ul>
<div class="tab-content">
    {% for account, data in accountData %}
        <div role="tabpanel" class="tab-pane {% if loop.first %}active{% endif %} loading-wrapper" id="raw-data-{{ loop.index0 }}">
            <h2>{{ account }}</h2>
            <table class="table table-striped">
                <thead>
                <th>#</th>
                <th>Unix timestamp</th>
                <th>Readable timestamp</th>
                <th>Source</th>
                <th>Account</th>
                <th>Type</th>
                <th>Content</th>
                </thead>
                <tbody>
                {% for row in data %}
                    <tr>
                        <td>{{ row.id }}</td>
                        <td>{{ row.timestamp }}</td>
                        <td>{{ row.timestamp|date }}</td>
                        <td>{{ row.source }}</td>
                        <td>{{ row.account }}</td>
                        <td>{{ row.type }}</td>
                        <td>{{ row.content }}</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    {% endfor %}
</div>
